@model IEnumerable<POC_Presentation_MVC.Models.ProductModel>

@Html.ActionLink("Create New", "Create", null, new { id="createNewjQuery", @class = "btn btn-primary" })
<br/><br />
<table id="productDataTable" class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Id)</th>
            <th>@Html.DisplayNameFor(model => model.Name)</th>
            <th>@Html.DisplayNameFor(model => model.Price)</th>
            <th>@Html.DisplayNameFor(model => model.Quantity)</th>
            <th>@Html.DisplayNameFor(model => model.CreationDate)</th>
            <th></th>
        </tr>
    </thead>
    <tbody></tbody>
</table>
<div id="renderModal"></div>